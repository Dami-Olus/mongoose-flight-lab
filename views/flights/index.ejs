<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Airline</th>
      <th>Airport</th>
      <th>FLight No.</th>
      <th>Departs</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(f){ %>
    <tr>
      <td><%= f.airline%></td>
      <td><%= f.airport%></td>
      <td><%= f.flightNumber%></td>
      <td><%= f.depature.toLocaleString()%></td>
      <td><a href="/flights/<%=f._id%>">Details</a></td>
    </tr>
    
    <% }) %>
  </tbody>
</table>
<%- include('../partials/footer') %>
